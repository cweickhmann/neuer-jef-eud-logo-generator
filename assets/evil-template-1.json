{
"config":
    {
    "outfile_prefix": "EVIL-LOGO_",
    "number_of_lines": 2,
    "text_line_string": "text_line",
    "bbox_padding":
        {
        "left":   0,
        "right":  0,
        "top":    0,
        "bottom": 0
        },
    "outputs":
        [
        "RGB",
        "blackandwhite_dark_background",
        "blackandwhite_bright_background"
        ],
